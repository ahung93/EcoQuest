<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.regular.ecoquest" >
    <uses-permission android:name="android.permission."/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />>
    <uses-feature android:name="android.hardware.camera" android:required="true" />

    <uses-permission android:name="android.permission.CAMERA"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TitleScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Challenge"
            android:label="@string/title_activity_challenge" >
        </activity>
        <activity
            android:name=".ChallengeOpen"
            android:label="@string/title_activity_challenge_open" >
        </activity>
        <activity
            android:name=".ChallengeListCurrent"
            android:label="@string/title_activity_challenge_list_current" >
        </activity>
    </application>

</manifest>
